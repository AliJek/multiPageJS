<!DOCTYPE html>
<html lang="pl">
	<head>
		<meta charset="UTF-8" />
		<meta http-equiv="X-UA-Compatible" content="IE=edge" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<title>Multi Javascript Page APP</title>
		<link rel="stylesheet" href="/css/style.css" />
		<script src="/src/js/main.js" defer></script>
	</head>
	<body>
		<!-- @include 'nav.kit' -->

		<main class="main-weather">
			<div class="wrapper">
				<div class="weatherapp">
					<h1>Weather App</h1>
					<div class="weatherapp__top">
						<input type="text" id="city" />
						<button class="weatherapp__top-btn">
							<img src="/src/img/search-svgrepo-com.svg" alt="" />
						</button>
					</div>
                    <p class="error"></p>
                    <h2 class="weatherapp__city"></h2>
					<div class="weatherapp__bottom">
						 <p class="temperature"></p>
						 <p class="weather"></p>
						 <p class="weather-img"></p>
					</div>
				</div>
			</div>
		</main>

		<script></script>
	</body>
</html>
